<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.github.ehsaniara</groupId>
  <artifactId>maven-repository-aws-s3</artifactId>
  <version>1.2.7</version>
  <packaging>maven-plugin</packaging>
  <name>Maven Repository with AWS S3</name>
  <description>Maven Repository with AWS S3 from S3 storage wagon.</description>
  <url>https://github.com/ehsaniara/maven-repository-aws-s3</url>
  <licenses>
    <license>
      <name>The Apache License, Version 2.0</name>
      <url>http://www.apache.org/licenses/LICENSE-2.0.txt</url>
    </license>
  </licenses>
  <developers>
    <developer>
      <name>Jay Ehsaniara</name>
      <email>ehsaniara@gmail.com</email>
      <organization>ehsaniara</organization>
      <organizationUrl>https://github.com/ehsaniara</organizationUrl>
    </developer>
  </developers>
  <scm>
    <connection>scm:git:https://github.com/ehsaniara/maven-repository-aws-s3.git</connection>
    <developerConnection>scm:git:https://github.com/ehsaniara/maven-repository-aws-s3.git</developerConnection>
    <url>http://github.com/ehsaniara/maven-repository-aws-s3</url>
  </scm>
  <issueManagement>
    <system>Github</system>
    <url>https://github.com/ehsaniara/maven-repository-aws-s3/issues</url>
  </issueManagement>
  <distributionManagement>
    <repository>
      <id>sonatype-nexus-repository</id>
      <url>https://oss.sonatype.org/service/local/staging/deploy/maven2</url>
    </repository>
    <snapshotRepository>
      <id>sonatype-nexus-repository</id>
      <url>https://oss.sonatype.org/content/repositories/snapshots</url>
    </snapshotRepository>
  </distributionManagement>
  <properties>
    <aws.version>1.11.743</aws.version>
    <java.version>11</java.version>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
  </properties>
